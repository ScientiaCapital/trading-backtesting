# ULTRA Trading Platform - Staging Environment Variables
# Copy this file to .env.staging and fill in your values
# DO NOT commit .env.staging to git

# Alpaca Paper Trading API (Staging)
ALPACA_API_KEY=your_staging_alpaca_api_key
ALPACA_API_SECRET=your_staging_alpaca_api_secret

# Authentication & Security
JWT_SECRET=your_jwt_secret_for_staging_at_least_32_characters
ENCRYPTION_KEY=your_encryption_key_for_staging_32_chars

# External Services
POLYGON_API_KEY=your_polygon_api_key_for_staging
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_for_staging

# AI Services (Optional)
OPENAI_API_KEY=your_openai_api_key_for_staging
ANTHROPIC_API_KEY=your_anthropic_api_key_for_staging

# Database & Analytics
POSTHOG_API_KEY=your_posthog_api_key_for_staging
SENTRY_DSN=your_sentry_dsn_for_staging

# Notifications (Optional)
SLACK_WEBHOOK_URL=your_slack_webhook_url_for_staging
DISCORD_WEBHOOK_URL=your_discord_webhook_url_for_staging
SENDGRID_API_KEY=your_sendgrid_api_key_for_staging

# Third-party Integrations
STRIPE_SECRET_KEY=your_stripe_secret_key_for_staging
TWILIO_AUTH_TOKEN=your_twilio_auth_token_for_staging

# Custom Domain (if using)
CUSTOM_DOMAIN=ultra-trading-staging.your-domain.com